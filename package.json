{
  "name": "@hyperplaza/sdk",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "description": "Hyperplaza NFT Marketplace SDK is an utilities for you to build your own NFT Marketplace, built with typescript ❤️, fully typed and tested. It is integrated with Serverless framework with AWS Lambda and AWS DynamoDB.",
  "repository": "git@github.com:airdropgames/nft-marketplace-sdk.git",
  "author": "Ahmad Ali Abdilah <ahmad.ali.abdilah@gmail.com>",
  "contributors": [
    {
      "email": "ahmad.ali.abdilah@gmail.com",
      "name": "Ahmad Ali Abdilah"
    },
    {
      "email": "nefti.dwiyan@gmail.com",
      "name": "Dwi",
      "url": "https://rashgaroth.tech"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/qs": "^6.9.7",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn test",
    "dev": "tsc --watch",
    "test": "jest",
    "clean": "rm -rf dist",
    "prepare": "husky install",
    "doc:html": "typedoc src/ --exclude **/*.test.ts --out docs",
    "serve:doc": "http-server docs -p 8080"
  },
  "engines": {
    "node": ">=12"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "dependencies": {
    "axios": "^1.4.0",
    "ethers": "5",
    "loglevel": "^1.8.1",
    "qs": "^6.11.2"
  }
}
