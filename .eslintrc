{
  "parser": "@typescript-eslint/parser",
  "plugins": ["import", "prettier", "unused-imports"],
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "semi": 1,
    "no-unused-vars": "off", // or "@typescript-eslint/no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error"
  }
}
